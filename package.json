{
  "name": "riversidebot",
  "version": "1.0.0",
  "description": "Discord bot for Riverside Discord server",
  "homepage": "https://github.com/FlussuferOrga/Botgart",
  "license": "ISC",
  "author": "Daniel O'Grady",
  "contributors": [
    {
      "name": "Daniel O'Grady",
      "url": "https://github.com/ogrady"
    },
    {
      "name": "Tobias GÃ¼nther",
      "url": "https://github.com/Xyaren"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FlussuferOrga/Botgart"
  },
  "bugs": {
    "url": "https://github.com/FlussuferOrga/Botgart/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "node ./node_modules/typescript/bin/tsc",
    "clean": "rimraf ./built",
    "start": "node ./built/index.js",
    "run": "npm-run-all clean build start",
    "test": "mocha",
    "lint": "eslint . --ext .ts,.json",
    "lint-fix": "eslint . --ext .ts,.json --fix",
    "dep-outdated": "npm outdated -l",
    "dep-typesync": "node ./node_modules/typesync/bin/typesync",
    "dep-check-unused": "node ./node_modules/depcheck/bin/depcheck.js --ignores='rimraf,better-sqlite3'",
    "dep-audit-fix": "npm audit fix",
    "typesafe-i18n": "typesafe-i18n",
    "orm-generate-entities": "mikro-orm generate-entities --save --path=./src/mikroorm/entities2",
    "orm-generate-migration": "mikro-orm migration:create",
    "orm-generate-migratio-initial": "mikro-orm migration:create --initial",

    "orm-check-migration": "mikro-orm migration:check"
  },
  "typesync": {
    "ignorePackages": [
      "eslint",
      "table"
    ]
  },
  "dependencies": {
    "better-sqlite3": "^8.4.0",
    "@mikro-orm/better-sqlite": "^5.7.12",
    "@mikro-orm/core": "^5.7.12",
    "@mikro-orm/migrations": "^5.7.12",
    "@notenoughupdates/discord-akairo": "^10.0.2",
    "@supercharge/promise-pool": "^2.4.0",
    "await-semaphore": "^0.1.3",
    "await-timeout": "^1.1.1",
    "callsites": "^3.1.0",
    "cheerio": "^1.0.0-rc.12",
    "command-line-args": "^5.2.1",
    "convict": "6.2.4",
    "discord.js": "^14.11.0",
    "gw2api-client": "^7.5.2",
    "lodash": "^4.17.21",
    "memoizer-ts": "^1.0.1",
    "moment": "^2.29.4",
    "moment-timezone": "0.5.43",
    "node-schedule": "^2.1.1",
    "server-health": "^5.2.0",
    "source-map-support": "^0.5.21",
    "table": "^6.8.1",
    "typesafe-i18n": "^5.24.3",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.7.12",
    "@types/await-timeout": "^0.3.1",
    "@types/chai": "^4.3.5",
    "@types/chai-datetime": "^0.0.37",
    "@types/chai-moment": "^0.1.1",
    "@types/command-line-args": "^5.2.0",
    "@types/convict": "^6.1.3",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/node-schedule": "^2.1.0",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "chai": "^4.3.7",
    "chai-datetime": "^1.8.0",
    "chai-moment": "^0.1.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "typesync": "^0.11.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikroorm/config.ts"
    ]
  }
}
