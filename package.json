  "main": "index.js",
  "scripts": {
    "build": "node ./node_modules/typescript/bin/tsc",
    "run": "npm run build && node ./built/index.js --patchall=run",
    "run-debug": "npm run build && node --trace-warnings ./built/index.js",
    "run-patchall": "npm run build && node ./built/index.js --patchall",
    "test": "mocha",
    "lint": "eslint . --ext .ts,.json",
    "lint-fix": "run lint --fix",
    "dep-outdated": "npm outdated",
    "dep-typesync": "node ./node_modules/typesync/bin/typesync",
    "dep-check-unused": "node ./node_modules/depcheck/bin/depcheck.js",
    "dep-audit-fix": "npm audit fix"
  },
  "typesync": {
    "ignorePackages": [
      "eslint",
      "table"
    ]
  },
  "dependencies": {
    "@supercharge/promise-pool": "^2.3.2",
    "await-semaphore": "^0.1.3",
    "await-timeout": "^1.1.1",
    "better-sqlite3": "^7.6.2",
    "callsites": "^3.1.0",
    "cheerio": "^1.0.0-rc.12",
    "command-line-args": "^5.2.1",
    "convict": "^6.2.3",
    "discord-akairo": "github:discord-akairo/discord-akairo#905f69382957023601ebbb6f8a3a8b6b0f615bd1",
    "discord.js": "^13.8.1",
    "gw2api-client": "^7.5.2",
    "lodash": "^4.17.21",
    "memoizer-ts": "^1.0.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "node-schedule": "^2.1.0",
    "server-health": "^5.1.1",
    "source-map-support": "^0.5.21",
    "table": "^6.8.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/await-timeout": "^0.3.1",
    "@types/better-sqlite3": "^7.6.0",
    "@types/chai": "^4.3.3",
    "@types/chai-datetime": "^0.0.37",
    "@types/chai-moment": "^0.1.1",
    "@types/command-line-args": "^5.2.0",
    "@types/convict": "^6.1.1",
    "@types/lodash": "^4.14.183",
    "@types/mocha": "^9.1.1",
    "@types/node-schedule": "^2.1.0",
    "@types/source-map-support": "^0.5.4",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.6",
    "chai-datetime": "^1.8.0",
    "chai-moment": "^0.1.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.23.0",
    "eslint-plugin-jsonc": "^2.2.1",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "typesync": "^0.9.2"
  }
