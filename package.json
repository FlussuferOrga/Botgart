  "repository": {
    "type": "git",
    "url": "https://github.com/FlussuferOrga/Botgart"
  },
  "bugs": {
    "url": "https://github.com/FlussuferOrga/Botgart/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "node ./node_modules/typescript/bin/tsc",
    "run": "npm run build && node ./built/index.js --patchall=run",
    "run-debug": "npm run build && node --trace-warnings ./built/index.js",
    "run-patchall": "npm run build && node ./built/index.js --patchall",
    "test": "mocha",
    "lint": "eslint . --ext .ts,.json",
    "lint-fix": "run lint --fix",
    "dep-outdated": "npm outdated",
    "dep-typesync": "node ./node_modules/typesync/bin/typesync",
    "dep-check-unused": "echo 'ignoring eslint until https://github.com/depcheck/depcheck/pull/641 is merged' && node ./node_modules/depcheck/bin/depcheck.js --ignores='eslint-plugin*,@typescript-eslint/*'"
  },
  "typesync": {
    "ignorePackages": [
      "eslint",
      "table"
    ]
  },
  "dependencies": {
    "@supercharge/promise-pool": "^1.7.0",
    "await-semaphore": "^0.1.3",
    "await-timeout": "^1.1.1",
    "better-sqlite3": "^7.4.1",
    "callsites": "^3.1.0",
    "cheerio": "^1.0.0-rc.10",
    "command-line-args": "^5.1.1",
    "convict": "^6.1.0",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.5.3",
    "gw2api-client": "^7.4.2",
    "lodash": "^4.17.21",
    "memoizer-ts": "^1.0.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node-schedule": "^2.0.0",
    "server-health": "^5.0.1",
    "source-map-support": "^0.5.19",
    "table": "^6.7.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/await-timeout": "^0.3.1",
    "@types/better-sqlite3": "^5.4.1",
    "@types/chai": "^4.2.19",
    "@types/chai-datetime": "^0.0.37",
    "@types/chai-moment": "^0.1.1",
    "@types/command-line-args": "^5.0.0",
    "@types/convict": "^6.0.2",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/node-schedule": "^1.3.1",
    "@types/source-map-support": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "chai": "^4.3.4",
    "chai-datetime": "^1.8.0",
    "chai-moment": "^0.1.0",
    "depcheck": "^1.4.1",
    "eslint": "^7.28.0",
    "eslint-plugin-jsonc": "^1.3.1",
    "mocha": "^9.0.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.3",
    "typesync": "^0.8.0"
  }
}
